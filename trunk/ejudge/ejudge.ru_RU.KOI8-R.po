msgid "<font color=\"red\">Rejected</font>"
msgstr "<font color=\"red\">Отказано</font>"

#, c-format
msgid "<h3>Warning %d: issued: %s, issued by: %s (%d), issued from: %s</h3>"
msgstr "<h3>Предупреждение %d: выдано: %s, пользователем: %s (%d), с: %s</h3>"

#, c-format
msgid "<i>Command line is too long</i>\n"
msgstr "<i>Командная строка слишком длинна</i>\n"

#, c-format
msgid "<i>Invalid answer: %d</i>"
msgstr "<i>Недопустимый ответ: %d</i>"

msgid "<i>Not set</i>"
msgstr "<i>Не установлено</i>"

#, c-format
msgid "<li>The contest is closed for participation.</li>\n"
msgstr "<li>Турнир закрыт для участия.</li>\n"

#, c-format
msgid ""
"<li>The session was created from a different IP-address or protocol, that "
"yours (%s,%s).</li>\n"
msgstr ""
"<li>Сессия была создана с другого адреса или по другому протоколу, чем те, "
"которые Вы используете в данный момент (%s,%s).</li>\n"

#, c-format
msgid "<li>The session was removed by an administrator.</li>"
msgstr "<li>Сессия была удалена администратором системы.</li>"

#, c-format
msgid "<li>The specified session does not exist.</li>\n"
msgstr "<li>Указанная сессия не существует.</li>\n"

#, c-format
msgid "<li>The specified session has expired.</li>\n"
msgstr "<li>Время действия сессии истекло.</li>\n"

#, c-format
msgid "<li>You are not registered for contest %d.</li>\n"
msgstr "<li>Вы не зарегистрированы на турнир %d.</li>\n"

#, c-format
msgid "<li>You are not registered for contest %s.</li>\n"
msgstr "<li>Вы не зарегистрированы на турнир %s.</li>\n"

#, c-format
msgid "<li>You do not have permissions to login using the specified role.</li>"
msgstr "<li>У Вас нет прав работать в указанной роли.</li>"

#, c-format
msgid "<li>You have typed an invalid login (<tt>%s</tt>).</li>\n"
msgstr "<li>Вы ввели неправильное регистрационное имя (<tt>%s</tt>).</li>\n"

#, c-format
msgid "<li>You have typed an invalid password.</li>\n"
msgstr "<li>Вы неправильно набрали пароль.</li>\n"

#, c-format
msgid "<li>You were banned by the administrator.</li>\n"
msgstr "<li>Ваше участие заблокировано администратором системы.</li>\n"

#, c-format
msgid ""
"<li>Your IP-address (<tt>%s</tt>) or protocol (<tt>%s</tt>) is banned for "
"participation.</li>"
msgstr ""
"<li>Ваш IP-адрес (<tt>%s</tt>) или протокол (<tt>%s</tt>) закрыт для участия."
"</li>"

#, c-format
msgid "<li>Your registration was not confirmed.</li>\n"
msgstr "<li>Ваша регистрация на турнир не подтверждена.</li>\n"

