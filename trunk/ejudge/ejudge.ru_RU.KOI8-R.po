#, c-format
msgid " / <b>%d unread message(s)</b>"
msgstr " / <b>%d непрочитанных сообщений</b>"

#, c-format
msgid " [%s, frozen]"
msgstr " [%s, заморожены]"

#, c-format
msgid " [over, frozen]"
msgstr " [окончен, заморожены]"

#, c-format
msgid " [over]"
msgstr " [окончен]"

msgid "\"E-mail\" field contains invalid characters"
msgstr "Поле \"Электронный адрес\" содержит недопустимые символы"

msgid "\"Login\" field contains invalid characters"
msgstr "Поле \"Регистрационное имя\" содержит недопустимые символы"

msgid "(no subject)"
msgstr "(без темы)"

msgid ", REGISTRATION DATA INCOMPLETE"
msgstr ", РЕГИСТРАЦИОННЫЕ ДАННЫЕ НЕПОЛНЫ"

msgid "/&lt;Problem&gt;/&lt;File&gt;"
msgstr "/&lt;Задача&gt;/&lt;Файл&gt;"

msgid "/&lt;Problem&gt;/&lt;User_Id&gt;/&lt;File&gt;"
msgstr "/&lt;Задача&gt;/&lt;Id_Польз.&gt;/&lt;Файл&gt;"

msgid "/&lt;Problem&gt;/&lt;User_Login&gt;/&lt;File&gt;"
msgstr "/&lt;Задача&gt;/&lt;Логин_Польз.&gt;/&lt;Файл&gt;"

msgid "/&lt;User_Id&gt;/&lt;File&gt;"
msgstr "/&lt;Id_Польз.&gt;/&lt;Файл&gt;"

msgid "/&lt;User_Id&gt;/&lt;Problem&gt;/&lt;File&gt;"
msgstr "/&lt;Id_Польз.&gt;/&lt;Задача&gt;/&lt;Файл&gt;"

msgid "/&lt;User_Login&gt;/&lt;File&gt;"
msgstr "/&lt;Логин_Польз.&gt;/&lt;Файл&gt;"

msgid "/&lt;User_Login&gt;/&lt;Problem&gt;/&lt;File&gt;"
msgstr "/&lt;Логин_Польз.&gt;/&lt;Задача&gt;/&lt;Файл&gt;"

#, c-format
msgid "<b>====== Test #%d =======</b>\n"
msgstr "<b>====== Тест #%d =======</b>\n"

msgid "<b>Note!</b> All uncommited changes will be lost!"
msgstr "<b>Внимание!</b> Все несохранённые изменения будут потеряны!"

msgid ""
"<b>Note</b>, that you must log in 24 hours after the form is filled and "
"submitted, or your registration will be cancelled!"
msgstr ""
"<b>Обратите внимание</b>, что Вы должны зайти в систему в течении 24 часов "
"после заполнения и посылки анкеты, или в противном случае Ваша регистрация "
"будет отменена!"

#, c-format
msgid "<big>%d total tests runs, %d passed, %d failed.<br>\n"
msgstr "<big>Всего тестов: %d, пройдено: %d, не пройдено: %d.<br>\n"

#, c-format
msgid "<big>Failed test: %d.<br><br></big>\n"
msgstr "<big>Ошибка на тесте %d.<br><br></big>\n"

msgid "<font color=\"green\">OK</font>"
msgstr "<font color=\"green\">OK</font>"

msgid "<font color=\"magenta\">Pending</font>"
msgstr "<font color=\"magenta\">Ожидает подтверждения</font>"

msgid "<font color=\"red\">Rejected</font>"
msgstr "<font color=\"red\">Отказано</font>"

#, c-format
msgid "<h3>Warning %d: issued: %s, issued by: %s (%d), issued from: %s</h3>"
msgstr "<h3>Предупреждение %d: выдано: %s, пользователем: %s (%d), с: %s</h3>"

#, c-format
msgid "<hr><big><b>You have %d unread message(s)!</b></big>\n"
msgstr "<hr><big><b>Непрочитанных сообщений: %d!</b></big>\n"

#, c-format
msgid "<i>Command line is too long</i>\n"
msgstr "<i>Командная строка слишком длинна</i>\n"

#, c-format
msgid "<i>Invalid answer: %d</i>"
msgstr "<i>Недопустимый ответ: %d</i>"

msgid "<i>Not set</i>"
msgstr "<i>Не установлено</i>"

#, c-format
msgid "<li>The contest is closed for participation.</li>\n"
msgstr "<li>Турнир закрыт для участия.</li>\n"

#, c-format
msgid ""
"<li>The session was created from a different IP-address or protocol, that "
"yours (%s,%s).</li>\n"
msgstr ""
"<li>Сессия была создана с другого адреса или по другому протоколу, чем те, "
"которые Вы используете в данный момент (%s,%s).</li>\n"

#, c-format
msgid "<li>The session was removed by an administrator.</li>"
msgstr "<li>Сессия была удалена администратором системы.</li>"

#, c-format
msgid "<li>The specified session does not exist.</li>\n"
msgstr "<li>Указанная сессия не существует.</li>\n"

#, c-format
msgid "<li>The specified session has expired.</li>\n"
msgstr "<li>Время действия сессии истекло.</li>\n"

#, c-format
msgid "<li>You are not registered for contest %d.</li>\n"
msgstr "<li>Вы не зарегистрированы на турнир %d.</li>\n"

#, c-format
msgid "<li>You are not registered for contest %s.</li>\n"
msgstr "<li>Вы не зарегистрированы на турнир %s.</li>\n"

#, c-format
msgid "<li>You do not have permissions to login using the specified role.</li>"
msgstr "<li>У Вас нет прав работать в указанной роли.</li>"

#, c-format
msgid "<li>You have typed an invalid login (<tt>%s</tt>).</li>\n"
msgstr "<li>Вы ввели неправильное регистрационное имя (<tt>%s</tt>).</li>\n"

#, c-format
msgid "<li>You have typed an invalid password.</li>\n"
msgstr "<li>Вы неправильно набрали пароль.</li>\n"

#, c-format
msgid "<li>You were banned by the administrator.</li>\n"
msgstr "<li>Ваше участие заблокировано администратором системы.</li>\n"

#, c-format
msgid ""
"<li>Your IP-address (<tt>%s</tt>) or protocol (<tt>%s</tt>) is banned for "
"participation.</li>"
msgstr ""
"<li>Ваш IP-адрес (<tt>%s</tt>) или протокол (<tt>%s</tt>) закрыт для участия."
"</li>"

#, c-format
msgid "<li>Your registration was not confirmed.</li>\n"
msgstr "<li>Ваша регистрация на турнир не подтверждена.</li>\n"

#, c-format
msgid ""
"<p class=\"ejudge_copyright\">This is <a href=\"%s\"><b>ejudge</b></a> "
"contest administration system, version %s, compiled %s.</p>\n"
"<p class=\"ejudge_copyright\">This program is copyright &copy; %s Alexander "
"Chernov.</p>\n"
"<p class=\"ejudge_copyright\">This program is free software; you can "
"redistribute it and/or modify it under the terms of the <a href=\"http://www."
"gnu.org/licenses/gpl.html\">GNU General Public License</a> as published by "
"the <a href=\"http://www.fsf.org\">Free Software Foundation</a>; either "
"version 2 of the License, or (at your option) any later version.</p>\n"
"<p class=\"ejudge_copyright\">Visual design and web-interface &copy; %s <a "
"href=\"%s\">Toto Lasvik</a>.</p>"
msgstr ""
"<p class=\"ejudge_copyright\">Это - система администрирования турниров <a "
"href=\"%s\"><b>ejudge</b></a>, версия %s, дата компиляции %s.</p>\n"
"<p class=\"ejudge_copyright\">Авторские права на программу: &copy; %s "
"Александр Чернов.</p>\n"
"<p class=\"ejudge_copyright\">Данная программа является свободным ПО. Вы "
"можете распространять и/или модифицировать её в соответствии с <a href="
"\"http://www.gnu.org/licenses/gpl.html\">Общей Лицензией GNU</a> (GNU "
"General Public License), опубликованной <a href=\"http://www.fsf.org"
"\">Фондом свободного ПО</a> (Free Software Foundation), либо версии 2 "
"Лицензии, либо (по Вашему желанию) любой последующей версии.</p>\n"
"<p class=\"ejudge_copyright\">Веб-дизайн и AJAX-интерфейс &copy; %s <a href="
"\"%s\">Toto Lasvik</a>.</p>"

#, c-format
msgid ""
"<p class=\"fixed_width\">First stage of password recovery is successful. You "
"should receive an e-mail message with further instructions. <b>Note,</b> "
"that you should confirm password recovery in 24 hours, or operation will be "
"cancelled.</p>"
msgstr ""
"<p class=\"fixed_width\">Первый этап восстановления пароля завершен. Теперь "
"Вы должны получить e-mail сообщение с дальнейшими инструкциями. <b>Учтите,</"
"b> что Вы должны завершить процедуру восстановления пароля в течение 24 "
"часов от данного момента, а иначе операция будет отменена.</p>"

#, c-format
msgid ""
"<p class=\"fixed_width\">Password recovery requires several steps. Now, "
"please, specify the <b>login</b> and the <b>e-mail</b>, which was specified "
"when the login was created.</p>\n"
"<p class=\"fixed_width\">Note, that automatic password recovery is not "
"possible for invisible, banned, locked, or privileged users!</p>\n"
msgstr ""
"<p class=\"fixed_width\">Восстановление пароля выполняется в несколько "
"этапов. Сейчас, пожалуйста, укажите <b>регистрационное имя</b> и <b>адрес e-"
"mail</b>, который был использован при создании этого регистрационного имени."
"</p>\n"
"<p class=\"fixed_width\">Учтите, что автоматическое восстановление пароля "
"невозможно для заблокированных, невидимых и привилегированных пользователей!"
"</p>\n"

#, c-format
msgid ""
"<p%s><a href=\"%s?contest_id=%d&amp;locale_id=%d&amp;action=%d\">Simplified "
"registration</a> is available for this contest. Note, however, that "
"simplified registration imposes certain restrictions on further use of the "
"account!</p>\n"
msgstr ""
"<p%s>Для данного турнира доступна <a href=\"%s?contest_id=%d&amp;locale_id=%"
"d&amp;action=%d\">упрощённая процедура регистрации</a>. Учтите, однако, что "
"на использование учётных записей, созданных по упрощённой процедуре, "
"наложены некоторые ограничения.</p>\n"

#, c-format
msgid ""
"<p%s><b>Note,</b> if downloads are allowed, you may download your runs once "
"per %d minutes. The archive is in <tt>.tar.gz</tt> (<tt>.tgz</tt>) format.</"
"p>\n"
msgstr ""
"<p%s><b>Обратите внимание,</b> что если загрузка пользовательских посылок "
"разрешена, вы можете делать это не чаще, чем один раз в %d минут. Архив "
"посылок имеет формат <tt>.tar.gz</tt> (<tt>.tgz</tt>).</p>\n"

#, c-format
msgid "<p%s><b>Remember or write down the password!</b></p>"
msgstr "<p%s><b>Запомните или запишите пароль!</b></p>"

#, c-format
msgid ""
"<p%s>Accounts created using simplified registration procedure cannot be used "
"for participation in contests, which do not allow simplified registration. "
"If you want a regular account, you may create an account using the <a href="
"\"%s?contest_id=%d&amp;locale_id=%d&amp;action=%d&amp;regular=1\">regular "
"registration</a>.</p>\n"
msgstr ""
"<p%s>Учётные записи, созданные по упрощённой процедуре, не могут быть "
"использованы для участия в турнирах, в которых не поддерживается упрощённая "
"регистрация. Если же Вы хотите создать учётную запись без ограничений на "
"последующее использование, воспользуйтесь <a href=\"%s?contest_id=%d&amp;"
"locale_id=%d&amp;action=%d&amp;regular=1\">стандартной процедурой "
"регистрации</a>.</p>\n"

#, c-format
msgid "<p%s>Click <a href=\"%s\">on this link</a> to login again.</p>\n"
msgstr ""
"<p%s>Нажмите <a href=\"%s\">на эту ссылку</a> для повторного входа.</p>\n"

#, c-format
msgid ""
"<p%s>Follow this <a href=\"%s?contest_id=%d%s\">link</a> to login again.</p>"
msgstr ""
"<p%s>Пойдите <a href=\"%s?contest_id=%d%s\">по этой ссылке</a> для "
"повторного входа.</p>"

#, c-format
msgid "<p%s>Hello, %s!</p>\n"
msgstr "<p%s>Здравствуйте, %s!</p>\n"

#, c-format
msgid ""
"<p%s>If automatic updating does not work, click on <a href=\"%s\">this</a> "
"link.</p>"
msgstr ""
"<p%s>Если автоматическое обновление не работает, нажмите <a href=\"%s\">на "
"эту</a> ссылку.</p>"

#, c-format
msgid ""
"<p%s>If you already have an ejudge account on this server, you may use it. "
"If so, follow the <a href=\"%s?contest_id=%d&amp;locale_id=%d&amp;action=%d"
"\">\"Use an existing account\"</a> link.</p>"
msgstr ""
"<p%s>Если у Вас уже есть учётная запись в системе ejudge на этом сервере, Вы "
"можете использовать её. Для этого проследуйте по ссылке <a href=\"%s?"
"contest_id=%d&amp;locale_id=%d&amp;action=%d\">\"Использовать существующую "
"учётную запись\"</a>.</p>"

#, c-format
msgid ""
"<p%s>If you are already registered, you may proceed to <a href=\"%s?"
"contest_id=%d&locale_id=%d\">personal page</a>.</p>"
msgstr ""
"<p%s>Если Вы уже зарегистрированы, Вы можете перейти на <a href=\"%s?"
"contest_id=%d&locale_id=%d\">личную страницу</a> для участия в турнире.</p>"

#, c-format
msgid ""
"<p%s>If you have not registered before, please <a href=\"%s\">register</a>.</"
"p>\n"
msgstr ""
"<p%s>Если Вы ещё не зарегистрировались, пожалуйста, <a href=\"%s"
"\">зарегистрируйтесь</a>.</p>\n"

#, c-format
msgid ""
"<p%s>New account <tt>%s</tt> is successfully created. An e-mail messages is "
"sent to the address <tt>%s</tt>. This message contains the password for the "
"initial log in. You will be able to change the password later.</p>\n"
"<p%s>Type the login and the password in to the form above and press the "
"\"Log in\" button to activate the account.</p>\n"
"<p%s><b>Note</b>, that you should log in to the system for the first time no "
"later, than in 24 hours after this moment, or the new account is removed.</p>"
msgstr ""
"<p%s>Новая учётная запись <tt>%s</tt> успешно создана. Теперь её нужно "
"активировать. Для этого на указанный Вами адрес электронной почты <tt>%s</"
"tt> послано письмо, которое содержит пароль для первого входа в систему. "
"Этот пароль в дальнейшем можно будет изменить.</p>\n"
"<p%s>Чтобы активировать учётную запись введите регистрационное имя и пароль "
"в форму вверху, затем нажмите кнопку \"Войти\".</p>\n"
"<p%s><b>Внимание</b>, Вы должны активировать новое регистрационное имя в "
"течение 24 часов с этого момента. В противном случае эта учётная запись "
"будет удалена.</p>"

#, c-format
msgid ""
"<p%s>New account <tt>%s</tt> is successfully created. Initial password is "
"generated automatically. You will be able to change your password later. "
msgstr ""
"<p%s>Новая учётная запись <tt>%s</tt> успешно создана. Начальный пароль для "
"входа сгенерирован автоматически. Вы сможете его поменять после входа в "
"систему. "

#, c-format
msgid ""
"<p%s>New account is successfully created. An e-mail messages is sent to the "
"address <tt>%s</tt>. This message contains the login name, assigned to you, "
"as well as your password for initial log in. You will be able to change the "
"password later.</p>\n"
"<p%s>Type the login and the password in to the form above and press the "
"\"Log in\" button to activate the account.</p>\n"
"<p%s><b>Note</b>, that you should log in to the system for the first time no "
"later, than in 24 hours after this moment, or the new account is removed.</p>"
msgstr ""
"<p%s>Новая учётная запись успешно создана. Теперь её нужно активировать. Для "
"этого на указанный Вами адрес электронной почты <tt>%s</tt> послано письмо, "
"которое содержит назначенное Вам регистрационное имя (login) и пароль для "
"первого входа в систему. Этот пароль в дальнейшем можно будет изменить.</p>\n"
"<p%s>Чтобы активировать учётную запись введите регистрационное имя и пароль "
"в форму вверху, затем нажмите кнопку \"Войти\".</p>\n"
"<p%s><b>Внимание</b>, Вы должны активировать новое регистрационное имя в "
"течение 24 часов от этого момента. В противном случае эта учётная запись "
"будет удалена.</p>"

#, c-format
msgid ""
"<p%s>New user account is created successfully. An initial password is "
"generated automatically, you will be able to change it later.\n"
"<p%s>The new account is as follows:\n"
msgstr ""
"<p%s>Учетная запись нового пользователя успешно создана. Начальный\n"
"пароль сгенерирован автоматически, Вы сможете поменять его позже.\n"
"<p%s>Параметры учетной записи следующие:\n"

#, c-format
msgid "<p%s>Page %d of %d.</p>\n"
msgstr "<p%s>Страница %d из %d.</p>\n"

#, c-format
msgid "<p%s>Press the &quot;Next&quot; button to continue registration."
msgstr "<p%s>Нажмите кнопку &quot;Дальше&quot; для продолжения регистрации."

#, c-format
msgid ""
"<p%s>Registration of a new user is completed successfully. An e-mail "
"messages is sent to the address <tt>%s</tt>. This message contains the login "
"name, assigned to you, as well as your password for initial login. To "
"proceed with registration, clink <a href=\"%s\">on this link</a>.</p><p%"
"s><b>Note</b>, that you should login to the system for the first time no "
"later, than in 24 hours after the initial user registration, or the "
"registration is void."
msgstr ""
"<p%s>Регистрация нового пользователя успешно завершена. На адрес <tt>%s</tt> "
"послано электронное письмо. Это сообщение содержит регистрационное имя, "
"присвоенное Вам, а также пароль для входа в систему. Чтобы продолжить "
"регистрацию, двигайтесь <a href=\"%s\">по этой ссылке</a>.</p><p%"
"s><b>Обратите внимание</b>, что Вы должны первый раз войти в систему в "
"течении 24 часов после первоначальной регистрации, а в противном случае "
"регистрация будет отменена."

#, c-format
msgid "<p%s>Remember or write down the password!."
msgstr "<p%s>Запомните или запишите пароль!"

#, c-format
msgid "<p%s>The account parameters are as follows:</p>\n"
msgstr "<p%s>Ваша регистрационная запись имеет следующие параметры:</p>\n"

#, c-format
msgid "<p%s>The current number of %s is %d.</p>\n"
msgstr "<p%s>Текущее количество %s - %d.</p>\n"

#, c-format
msgid "<p%s>The maximal number of %s is %d.</p>\n"
msgstr "<p%s>Максимальное количество %s - %d.</p>\n"

#, c-format
msgid "<p%s>The minimal number of %s is %d.</p>\n"
msgstr "<p%s>Минимальное количество %s - %d.</p>\n"

#, c-format
msgid ""
"<p%s>This contest operates in \"simplified registration\" mode. You will get "
"your login and password immediately after account is created. %s</p>"
msgstr ""
"<p%s>В этом турнире включена упрощённая процедура регистрации. Вы получите "
"пароль от своей учётной записи немедленно после её создания (без "
"использования электронной почты). %s</p>"

#, c-format
msgid ""
"<p><font color=\"red\">Maximal number for this contest is %d, but already %d "
"are defined.</font></p>\n"
msgstr ""
"<p><font color=\"red\">Максимальное число участников для этого турнира равно "
"%d, но уже определено %d.</font></p>\n"

#, c-format
msgid ""
"<p><font color=\"red\">Minimal number for this contest is %d, but only %d "
"are defined.</font></p>\n"
msgstr ""
"<p><font color=\"red\">Минимальное число членов команды в этом турнире %d, "
"но определено только %d.</font></p>\n"

#, c-format
msgid "<p>Information is not available.</p>"
msgstr "<p>Информация отсутствует.</p>"

#, c-format
msgid "<p>New password is generated.</p>"
msgstr "<p>Новый пароль сгенерирован.</p>"

#, c-format
msgid ""
"<p>Note, that the exact reason is not reported due to security reasons.</p>"
msgstr ""
"<p>Обратите внимание, что точная причина ошибки не сообщается по "
"соображениям безопасности.</p>"

#, c-format
msgid "<p>You may define up to %d members.</p>"
msgstr "<p>Вы можете добавить до %d членов команды.</p>"

#, c-format
