DCCRUN="@ac_cv_contest_dcc@"
DCCDIR="@ac_cv_contest_dcc_dir@"

if [ x"${DCCRUN}" = x -o x"${DCCRUN}" = xno ]
then
  echo "This language is not supported." >&2
  exit 1
fi

if [ x"${DCCDIR}" != x ]
then
    PATH="${DCCDIR}:${PATH}"
    KYLIXDIR="${DCCDIR}"
    export KYLIXDIR
fi

exec "${DCCRUN}" -$I+,Q-,R-,X+ -Q "$1"
