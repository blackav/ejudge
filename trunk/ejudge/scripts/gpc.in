# using EJUDGE_FLAGS we may pass additional flags

GPCRUN="@ac_cv_contest_gpc@"
GPCDIR="@ac_cv_contest_gpc_dir@"

if [ x"${GPCRUN}" = x -o x"${GPCRUN}" = xno ]
then
  echo "This language is not supported." >&2
  exit 1
fi

if [ x"${GPCDIR}" != x ]
then
    PATH="${GPCDIR}:${PATH}"
fi

exec "${GPCRUN}" ${EJUDGE_FLAGS} -static -s -O2 "$1" -o "$2"
