GCCRUN="@ac_cv_contest_gcc@"
GCCDIR="@ac_cv_contest_gcc_dir@"

if [ x"${GCCRUN}" = x -o x"${GCCRUN}" = xno ]
then
  echo "This language is not supported." >&2
  exit 1
fi

if [ x"${GCCDIR}" != x ]
then
    PATH="${GCCDIR}:${PATH}"
fi

exec "${GCCRUN}" -Wall -static -s -O2 "$1" -o "$2" -lm
