#!/bin/sh
# $Id$
# A dumb script to report "version" for Plain Text "language"
# Copyright (c) 2006 Alexander Chernov <cher@unicorn.cmc.msu.ru>

TXTRUN="/bin/false"

if [ x"${TXTRUN}" = x -o x"${TXTRUN}" = xno ]
then
  echo "This language is not supported." >&2
  exit 1
fi

if [ x"$1" = x-p ]
then
    echo "${TXTRUN}"
    exit 0
fi

[ x"$1" = x-f ] && echo -n "Plain Text Document - "

echo "Any Version"
exit 0
