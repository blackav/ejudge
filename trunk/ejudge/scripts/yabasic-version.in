YABASICPATH="@ac_cv_contest_yabasic_path@"

if [ x"${YABASICPATH}" = x -o x"${YABASICPATH}" = xno ]
then
  echo "This language is not supported." >&2
  exit 1
fi

if [ x"$1" = x-p ]
then
    echo "${YABASICPATH}"
    exit 0
fi
    
[ x"$1" = x-f ] && echo -n "YaBasic "

"${YABASICPATH}" -help 2>&1 | grep "yabasic version" | gawk -F '[ ,]' '{ print $5; }'
