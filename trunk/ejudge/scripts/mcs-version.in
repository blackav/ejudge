MCSRUN="@ac_cv_contest_mcs@"
MCSDIR="@ac_cv_contest_mono_dir@"

if [ x"${MCSRUN}" = x -o x"${MCSRUN}" = xno ]
then
  echo "This language is not supported." >&2
  exit 1
fi

if [ x"${MCSDIR}" != x ]
then
  PATH="${MCSDIR}/bin:${PATH}"
fi

if [ x"$1" = x-p ]
then
    echo "${MCSRUN}"
    exit 0
fi

"${MCSRUN}" --version 2>/dev/null >/dev/null || exit 1
    
[ x"$1" = x-f ] && echo -n "Mono C# "

"${MCSRUN}" --version 2>&1 | grep "version" | awk '{gsub(",","",$5); print $5;}'
