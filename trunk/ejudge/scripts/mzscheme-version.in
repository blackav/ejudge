MZSCHEMEPATH="@ac_cv_contest_mzscheme_path@"

if [ x"${MZSCHEMEPATH}" = x -o x"${MZSCHEMEPATH}" = xno ]
then
  echo "This language is not supported." >&2
  exit 1
fi

[ x"$1" = x-f ] && echo -n "Mz Scheme "

"${MZSCHEMEPATH}" --version 2>&1 | grep "MzScheme version" | gawk -v FS="[ ,]" '{ print $5; }'
