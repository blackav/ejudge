GPPRUN="@ac_cv_contest_gpp@"
GPPDIR="@ac_cv_contest_gpp_dir@"

if [ x"${GPPRUN}" = x -o x"${GPPRUN}" = xno ]
then
  echo "This language is not supported." >&2
  exit 1
fi

if [ x"${GPPDIR}" != x"" -o x"${GPPDIR}" != xno ]
then
    PATH="${GPPDIR}:${PATH}"
fi

exec "${GPPRUN}" -Wall -static -s -O2 "$1" -o "$2" -lm
