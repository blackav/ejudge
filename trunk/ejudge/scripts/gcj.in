# If GNU Compiler Collection is installed at non-standard place
# GCCDIR=/usr/upd/gcc-3.3.2

GCCRUN=gcj
if [ "${GCCDIR}" != "" ]
then
    PATH=${GCCDIR}/bin:${PATH}
    GCCRUN=${GCCDIR}/bin/${GCCRUN}
fi

mv $1 Main.java
exec ${GCCRUN} --main=Main -static -s -O2 Main.java -o $2
