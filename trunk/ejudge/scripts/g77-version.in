G77RUN="@ac_cv_contest_g77@"
G77DIR="@ac_cv_contest_g77_dir@"

if [ x"${G77RUN}" = x -o x"${G77RUN}" = xno ]
then
  echo "This language is not supported." >&2
  exit 1
fi

if [ x"${G77DIR}" != x ]
then
    PATH="${G77DIR}:${PATH}"
fi

[ x"$1" = x-f ] && echo -n "GNU Fortran 77 "

"${G77RUN}" --version | grep "GNU Fortran (GCC)" | gawk '{ print $4; }'
[ $? != 0 -a $? != 1 ] && exit 1
exit 0
