GCJRUN="@ac_cv_contest_gcj@"
GCJDIR="@ac_cv_contest_gcj_dir@"

if [ x"${GCJRUN}" = x -o x"${GCJRUN}" = xno ]
then
  echo "This language is not supported." >&2
  exit 1
fi

if [ x"${GCJDIR}" != x ]
then
    PATH="${GCJDIR}:${PATH}"
fi

if [ x"$1" = x-p ]
then
    echo "${GCJRUN}"
    exit 0
fi
    
[ x"$1" = x-f ] && echo -n "GNU Java (GCJ) "

"${GCJRUN}" --version | grep gcj | gawk '{ print $3; }'
[ $? != 0 -a $? != 1 ] && exit 1
exit 0
