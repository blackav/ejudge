# using EJUDGE_FLAGS we may pass additional flags

G77RUN="@ac_cv_contest_g77@"
G77DIR="@ac_cv_contest_g77_dir@"

if [ x"${G77RUN}" = x -o x"${G77RUN}" = xno ]
then
  echo "This language is not supported." >&2
  exit 1
fi

if [ x"${G77DIR}" != x ]
then
    PATH="${G77DIR}:${PATH}"
fi

exec "${G77RUN}" ${EJUDGE_FLAGS} -static -s -O2 "$1" -o "$2"
