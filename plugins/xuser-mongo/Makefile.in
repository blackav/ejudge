prefix=@prefix@
exec_prefix=@exec_prefix@
bindir=@bindir@
datarootdir=@datarootdir@
datadir=@datadir@
includedir=@includedir@
libdir=@libdir@
libexecdir=@libexecdir@

EXPAT_DIR=@ac_cv_expat_root@
EXPAT_INCL_OPT=@ac_cv_expat_include_opt@
EXPAT_LIB_OPT=@ac_cv_expat_lib_opt@

MONGOC_EXISTS=@ac_cv_has_libmongoc@
MONGOC_CFLAGS=@ac_cv_libmongoc_cflags@
MONGOC_LIBS=@ac_cv_libmongoc_libs@

WPTRSIGN=@ac_cv_gcc_wno_pointer_sign@ @ac_cv_gcc_wno_format_truncation@
WERROR=@ac_cv_werror_flag@

ifdef RELEASE
CDEBUGFLAGS=-O2 -Wall -DNDEBUG -DRELEASE ${WERROR}
else
CDEBUGFLAGS=-g -Wall ${WERROR}
endif
ifdef STATIC
CDEBUGFLAGS += -static
endif
CEXTRAFLAGS=
LDEXTRAFLAGS=
EXTRALIBS=
CCOMPFLAGS=-D_GNU_SOURCE -std=gnu11
LDCOMPFLAGS=

ifneq ($(MONGOC_EXISTS),)
include main.make
else
include empty.make
endif
